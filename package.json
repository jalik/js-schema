{
  "name": "@jalik/schema",
  "version": "4.2.0",
  "description": "A utility to validate complex objects using schemas",
  "license": "MIT",
  "keywords": [
    "check",
    "clean",
    "json",
    "object",
    "schema",
    "validation"
  ],
  "author": {
    "name": "Karl Stein",
    "email": "karl.stein.pro@gmail.com",
    "url": "https://github.com/jalik"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jalik/js-schema.git"
  },
  "bugs": {
    "url": "https://github.com/jalik/js-schema/issues"
  },
  "sideEffects": false,
  "esnext": "./esm/index.js",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "files": [
    "/dist",
    "/esm"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "tsc -p tsconfig.cjs.json",
    "build-esm": "tsc",
    "clean": "rimraf ./dist ./esm",
    "dev-cjs": "tsc --watch -p tsconfig.cjs.json",
    "dev-esm": "tsc --watch",
    "dev": "concurrently 'npm:dev-*'",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --ext .ts --fix .",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "test": "jest --passWithNoTests --coverage=true"
  },
  "dependencies": {
    "@jalik/deep-extend": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.6.3"
  }
}
